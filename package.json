  "scripts": {
    "webpack-dev-server": "webpack-dev-server",
    "start": "webpack-dev-server --open --mode=development",
    "build": "webpack --config webpack.config.js --mode development",
    "build-prod": "webpack --config webpack.config.js --mode production",
    "test-watch": "react-scripts test",
    "test": "react-scripts test --watchAll=false --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --quiet",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,css,md}'",
    "type-check": "tsc --noEmit --skipLibCheck",
    "prebuild": "yarn run lint && yarn run type-check",
    "postbuild": "echo 'Build completed!'",
    "full-build": "yarn run prebuild && yarn run build-prod && yarn run postbuild",
    "analyze": "cross-env ANALYZE=true yarn build",
    "patch-version": "yarn run full-build && yarn test && standard-version && git push --follow-tags"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 80
